package javatraining;

public class ReverseWithoutUsingReverseMethod {
	
	
	public static void main(String args[])
	{
		
		String Str="Amrit Pandita";
		
		char char1[]=Str.toCharArray();
		char char2[]=null;
		int j=0;
		for(int i=Str.length()-1;i>=0;i--)
		{
			
			char2[j]=char1[i];
			
			
			
		}
		
		
		
	}

}
